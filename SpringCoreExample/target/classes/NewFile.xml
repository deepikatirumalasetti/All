<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="address" class="Assignment1.Address">
<property name="City" value="Tadepalligudem"></property>
<property name="State" value="AndhraPradesh"></property>
<property name="Street" value="Bhagyalakshmipet"></property>
<property name="Zip" value="12345"></property>
<property name="Country" value="India"></property>
</bean>
<bean id="obj" class="Assignment1.Customer">
<property name="id" value="1"></property>
<property name="name" value="jyo"></property>
<property name="contact" value="8688574328"></property>
<property name="address" ref="Street"></property>
</bean>
</beans>